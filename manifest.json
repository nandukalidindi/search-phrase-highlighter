{
  "manifest_version": 2,
  "name": "Search Phrase Highlighter",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.3.min.js", "jquery.mark.min.js", "highlighter.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
